<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <Logentries ssl="false">
        <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss ZZZ} %F.%L level:%-5p%m"/>
        <name>le</name>
    </Logentries>
</Appenders>

<Loggers>
    <Root level="INFO">
        <AppenderRef ref="le" />
    </Root>
</Loggers>
</Configuration>